{
  "name": "photo-gallery",
  "private": true,
  "version": "1.3.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "build-server": "cd server && CGO_ENABLED=1 go build -v -installsuffix cgo -o photo-gallery",
    "preview": "vite preview",
    "server": "cd server && go build && ./photo-gallery",
    "server-dev": "nodemon --ext go --watch './server/' --signal SIGTERM --exec 'cd server && go build && ./photo-gallery'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/pt-sans": "^4.5.11",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@reduxjs/toolkit": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/react-dom": "^18.0.11",
    "leaflet": "^1.9.3",
    "notistack": "v3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-photo-album": "^2.0.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-virtualized-auto-sizer": "^1.0.16",
    "react-window": "^1.8.9",
    "typescript": "^4.9.5",
    "yet-another-react-lightbox": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/leaflet": "^1.9.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-window": "^1.8.5",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.38.0",
    "eslint-config-react-app": "^7.0.1",
    "nodemon": "^2.0.22",
    "sass": "^1.62.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
